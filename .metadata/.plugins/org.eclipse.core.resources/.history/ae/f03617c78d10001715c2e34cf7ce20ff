package algorithms.string;

import java.util.Scanner;

public class SeparateTheNumbers {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int N = in.nextInt();
        for(int i=0; i<N; i++){
            String s = in.next();
            
            String sub = s.substring(0, 1);
            System.out.println(sub);
            
            
            
            
            
            
        }
        in.close();
    }
}
