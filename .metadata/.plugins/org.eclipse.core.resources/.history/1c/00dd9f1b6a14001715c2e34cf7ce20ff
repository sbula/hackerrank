package algorithms.constructiveAlgorithms;

import java.util.Arrays;
import java.util.Scanner;

public class Bonetrousle {

    public static void main(String[] args) {
    	Scanner in = new Scanner(System.in);
    	int trips = in.nextInt();
    	for (int i=0; i<trips; i++) {
    		int sticks = in.nextInt();
    		int stored = in.nextInt();
    		int boxes = in.nextInt();
    		
    		int availableSticks = stored * (stored+1)/2;
    		int maxSticksBuy = stored-(stored-boxes) * (stored-boxes+1)/2;
    		int minSticksBuy = (boxes) * (boxes+1)/2;
    		
    		if (stored<boxes || (availableSticks<sticks || maxSticksBuy<sticks || minSticksBuy>sticks)) {
    			System.out.println(-1);
    			continue;
    		}
    		
    		int[] buyList = new int[boxes];
    		for(int k=0; k<buyList.length; k++) {
    			buyList[k] = k+1;
    		}
    		int sticksInBasket = minSticksBuy;
    		int xChgInd = 0;
    		int minAvailable = boxes+1;
    		int maxAvailable = stored;
    		while(sticksInBasket<sticks) {
    			int diff = sticks - sticksInBasket;
    			if (diff<maxAvailable-buyList[xChgInd]) {
    				if (minAvailable<buyList[xChgInd]+diff) {
    					buyList[xChgInd] += diff; 
    				} else {
    					xChgInd++;
    				}
    			} else {
    				sticksInBasket += maxAvailable-buyList[xChgInd];
    				buyList[xChgInd] = maxAvailable;
    				maxAvailable--;
    				xChgInd++;
    			}
    		}
    		
    		Arrays.sort(buyList);
    		for (int n=0; n<buyList.length; n++) {
    			System.out.print(buyList[n] + " ");
    		}
    		System.out.println();
    	}
    	in.close();
    }

}
