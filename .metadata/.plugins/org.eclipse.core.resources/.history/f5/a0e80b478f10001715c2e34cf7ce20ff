package algorithms.string;

import java.util.Scanner;

public class SeparateTheNumbers {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int N = in.nextInt();
        for(int i=0; i<N; i++){
            String s = in.next();
            
            int start = 0, diff = 1;
            while (start+diff<=(s.length()+1)/2) {
            	String sub = s.substring(start, diff);
            	System.out.print(sub + " ");
            	int nextInt = Integer.getInteger(sub) + 1;
            	String nextNumber = Integer.toString(nextInt);
            	System.out.println(nextNumber);
            	diff++;
            }
        }
        in.close();
    }
}
